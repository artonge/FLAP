{
	"name": "flap-manager",
	"version": "0.0.1",
	"description": "A manager for FLAP boxes.",
	"main": "build/main.js",
	"scripts": {
		"test": "jest --forceExit --coverage --verbose",
		"test:watch": "jest --watch",
		"start": "NODE_ENV=production node build/main.js",
		"build": "tsc",
		"serve": "LOG_LEVEL=silly concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w --removeComments false\" \"nodemon --inspect build/main.js\"",
		"debug": "LOG_LEVEL=silly DEBUG=express:* concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w --removeComments false\" \"nodemon --inspect build/main.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/artonge/flap.git"
	},
	"keywords": [
		"flap",
		"core"
	],
	"author": "artonge",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://gitlab.com/artonge/flap/issues"
	},
	"homepage": "https://gitlab.com/artonge/flap#readme",
	"dependencies": {
		"cheerio": "^1.0.0-rc.2",
		"core-js": "^3.0.0-beta.15",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.10",
		"@types/jest": "^24.0.5",
		"concurrently": "^4.1.0",
		"jest": "^24.1.0",
		"nodemon": "^1.18.9",
		"ts-jest": "^23.10.5",
		"typescript": "^3.2.2"
	}
}
