{
	"compilerOptions": {
		"target": "es5",
		"rootDir": "./src",
		"outDir": "./build",
		"sourceMap": true,
		"removeComments": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"lib": ["ESNext"]
	}
}
